<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">


	<configuration>
		<custom-converters>
			<converter type="introsde.assignment.util.DateTimeConverter">
				<class-a>java.util.Date</class-a>
				<class-b>org.joda.time.DateTime</class-b>
			</converter>
		</custom-converters>
	</configuration>
	<mapping>
		<class-a>introsde.assignment.soap.bean.MeasureBean</class-a>
		<class-b>introsde.assignment.soap.model.Measure</class-b>
		<field>
			<a>dateRegistered</a>
			<b>dateRegistered</b>
		</field>
		<field>
			<a>measureValueType</a>
			<b>measureValueType</b>
		</field>
		<field>
			<a>measureType</a>
			<b>measureType</b>
		</field>
		<field>
			<a>mid</a>
			<b>mid</b>
		</field>
	</mapping>
	<mapping type="one-way" wildcard="true">
		<class-a>introsde.assignment.soap.model.Person</class-a>
		<class-b>introsde.assignment.soap.bean.PersonBean</class-b>

		<field>
			<a get-method="getListMeasureByPerson" map-get-method="getListMeasureByPerson"></a>
			<b>healthHistory</b>
		</field>
		<field>
			<a>firstname</a>
			<b>firstname</b>
		</field>
		<field>
			<a>lastname</a>
			<b>lastname</b>
		</field>
		<field>
			<a get-method="getCurrentMeasureByPerson" map-get-method="getListMeasureByPerson"></a>
			<b>currentHealth</b>
		</field>
		<field>
			<a>id</a>
			<b>id</b>
		</field>
	</mapping>
	<mapping></mapping>
	<mapping></mapping>
</mappings>
        